
<div class="container">
   <h1>Additional Detail</h1>
   <% if @teammember.errors.any? %>
    <ol>
      <% @teammember.errors.full_messages.each do |msg| %>
        <li><%=  msg %></li>
      <% end %>
    </ol>
  <% end %>

          <%= bootstrap_form_for @teammember do |f| %>
            <%= f.text_field :first_name, :required => true %>
            <%= f.text_field :last_name, :required => true %>
            <%= f.text_field :preferred_first_name, :required => true %>
            <%= f.password_field :password_digest, :required => true %>
            <%= f.password_field :password_confirmation, :required => true %>
            <%= f.url_field :profile_potrait %>
            <%= f.submit "Next" %>
          <% end %>
</div>
